Xterm.termName: xterm-256color

XTerm.vt100.visualBell: true
XTerm.vt100.background: black
XTerm.vt100.foreground: grey
!XTerm.vt100.faceName: Iosevka:size=10:antialias=true
!XTerm.vt100.faceName: Hack:size=10:antialias=true
!XTerm.vt100.faceName: Terminus:size=10:antialias=true
!XTerm.vt100.faceName: VGA:size=10:antialias=true
XTerm.vt100.faceName: Source Code Pro:size=10:antialias=true
XTerm.vt100.saveLines: 999999

Xcursor.size: 18

XTerm.vt100.utf8: true
XTerm.vt100.translations: #override \n\
    Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\

Xcursor.theme: Adwaita
XTerm.vt100.metaSendsEscape: true

! special
*.foreground:   #586e75
*.background:   #fff8e5
*.cursorColor:  #586e75

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #9c988c
